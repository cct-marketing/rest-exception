{
    "name": "cct-marketing/rest-exception-handler",
    "description": "An exception handler used to handle and parse all errors from the rest lib requests",
    "type": "library",
    "require": {
        "php": "~7.4 | ~8.0",
        "guzzlehttp/guzzle": "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "CCT\\Component\\RestExceptionHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCT\\Component\\RestExceptionHandler\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Schweppe",
            "email": "ps@cct-marketing.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
